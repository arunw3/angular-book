{
  "name": "flug-portal",
  "version": "1.0.0",
  "description": "Example Project",
  "license": "MIT",
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "webpack --env=prod --progress",
    "build:aot": "npm run ngc && webpack --env=aot --progress",
    "build:aot-german": "npm run ngc:german && webpack --env=aot-german --progress",
    "build:aot-plugin": "webpack --env=aot-plugin --progress",
    "build:all": "npm run build && npm run build:prod && npm run build:aot && npm run build:aot-german && npm run build:aot-plugin",
    "webpack": "webpack",
    "start": "webpack-dev-server --env=dev",
    "start:no-animation": "webpack-dev-server --env=no-animation",
    "test": "karma start ./karma.conf.js",
    "test:single": "karma start ./karma.conf.js --single-run true",
    "tsc": "tsc",
    "lint": "tslint \"src/**/*.ts\"",
    "protractor": "protractor",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor protractor.conf.js",
    "e2e:start": "concurrently -s first -k --names \"DEV-SERVER,E2E-TEST\" --prefix \"[{name}]\" \"npm run -s start:no-animation\" \"npm run e2e\"",
    "translate": "ng-xi18n -p src/tsconfig.app.json --i18nFormat=xlf",
    "ngc": "ngc -p tsconfig.aot.json",
    "ngc:german": "ngc -p tsconfig.aot.json --i18nFile=src/locale/messages.de.xlf --locale=de --i18nFormat=xlf",
    "check": "npm run test:single && npm run e2e:start && npm run translate && npm run build:all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.2.4",
    "@angular/common": "4.2.4",
    "@angular/compiler": "4.2.4",
    "@angular/core": "4.2.4",
    "@angular/forms": "4.2.4",
    "@angular/http": "4.2.4",
    "@angular/platform-browser": "4.2.4",
    "@angular/platform-browser-dynamic": "4.2.4",
    "@angular/router": "4.2.4",
    "@angular/upgrade": "4.2.4",
    "@ngx-translate/core": "7.0.0",
    "@ngx-translate/http-loader": "0.1.0",
    "angular-oauth2-oidc": "1.0.20",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "rxjs": "5.4.1",
    "web-animations-js": "2.2.5",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.2.4",
    "@ngtools/webpack": "1.4.2",
    "@types/jasmine": "2.5.42",
    "@types/node": "8.0.3",
    "angular-router-loader": "0.6.0",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "codelyzer": "3.1.1",
    "compression-webpack-plugin": "0.4.0",
    "concurrently": "3.5.0",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-remap-istanbul": "0.6.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "mkdirp": "0.5.1",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "ts-node": "3.1.0",
    "tslint": "5.4.3",
    "tslint-loader": "3.5.3",
    "typescript": "2.3.4",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0",
    "webpack-merge": "4.1.0"
  },
  "homepage": "http://angular.at"
}
